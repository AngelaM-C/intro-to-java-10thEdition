package chapter7;

public class EightQueens {

	public static void main(String[] args) {
	
		String[][] array ={{"|","Q","|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|"},
							   {"|"," ","|"," ","|"," ","|"," ","|","Q","|"," ","|"," ","|"," ","|"},
							   {"|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|","Q","|"},
							   {"|"," ","|"," ","|"," ","|"," ","|"," ","|","Q","|"," ","|"," ","|"},
							   {"|"," ","|"," ","|","Q","|"," ","|"," ","|"," ","|"," ","|"," ","|"},
							   {"|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|","Q","|"," ","|"},
							   {"|"," ","|","Q","|"," ","|"," ","|"," ","|"," ","|"," ","|"," ","|"},
							   {"|"," ","|"," ","|"," ","|","Q","|"," ","|"," ","|"," ","|"," ","|"}};
		
		 for(int i=0;i<array.length;i++) {
			 for(int j=0; j<array[i].length; j++) 
				 System.out.print(array[i][j]);
			 
			 System.out.println();
			   
		}
	}

}
